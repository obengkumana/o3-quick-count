case $2 in
	1|01)
		echo "$3 Januari $1"
	;;
	2|02)
		echo "$3 Februari $1"
	;;
	3|03)
		echo "$3 Maret $1"
	;;
	4|04)
		echo "$3 April $1"
	;;
	5|05)
		echo "$3 Mei $1"
	;;
	6|06)
		echo "$3 Juni $1"
	;;
	7|07)
		echo "$3 Juli $1"
	;;
	8|08)
		echo "$3 Agustus $1"
	;;
	9|09)
		echo "$3 September $1"
	;;
	10)
		echo "$3 Oktober $1"	
	;;
	11)
		echo "$3 November $1"
	;;
	12)
		echo "$3 Desember $1"
	;;
esac